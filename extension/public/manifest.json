{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "short_name": "TabDash",
  "version": "0.2.7",
  "description": "__MSG_extensionDesc__",
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "index.html"
