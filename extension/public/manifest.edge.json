{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "short_name": "TabDash",
  "version": "0.2.9",
  "description": "__MSG_extensionDesc__",
  "author": "Wesley Urban",
  "homepage_url": "https://tabdash.wesley.fyi",
  "default_locale": "en",
  "icons": {
    "16": "tabdash_16.png",
    "48": "tabdash_48.png",
    "128": "tabdash_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module",
    "offline_enabled": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "geolocation",
    "search",
    "clipboardRead"
  ],
  "action": {
    "browser_style": true,
    "default_icon": "tabdash_128.png",
    "default_title": "__MSG_extensionName__"
  }
}